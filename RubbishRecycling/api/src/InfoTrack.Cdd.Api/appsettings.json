            {
                "PolicyName": "_allow-all",
                "AllowAnyOrigin": true,
                "WithOrigins": [],
                "AllowAnyHeader": true,
                "WithHeaders": [],
                "AllowAnyMethod": true,
                "WithMethods": [],
