    "Serilog": {
        "Using": ["Serilog.Sinks.ElasticSearch"],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo": [
            {
                "name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            },
            {
                "name": "Console",
                "Args": {
                    "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                    "outputTemplate":
                        "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
                }
            },
            {
                "Name": "Elasticsearch",
                "Args": {
                    "nodeUris": "https://logs.infotrack.com.au",
                    "indexFormat": "cdd-{0:yyyy.MM}",
                    "connectionGlobalHeaders": "Authorization=Basic bG9nX3dyaXRlcjppcFhwak5EMHR0V0dpRVhMN0t3ME5rQ2E="
                }
            }
        ],
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression":
                        "@Properties['SourceContext'] = 'Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware'"
                }
            }
        ],
        "Enrich": ["WithMachineName", "FromLogContext", "Environment", "Application", "Region"],
        "Properties": {
            "Environment": "Development",
            "Application": "Infotrack.Cdd",
            "Region": "AU"
        }
    }
}
